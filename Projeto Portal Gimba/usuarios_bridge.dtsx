                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- Selecionar Compradores
select
u.IdLogin,
u.Email,
upper(u.Nome) as Nome

from
Usuarios u
join UsuarioCliente uc on
u.IdLogin = uc.IdLogin
join Clientes c on
uc.IdCliente = c.IdCliente
join ContratoCliente cc on
u.IdClienteResponsavel = cc.IdCliente
where cc.Ativo = 'S' and u.IdPerfilUsuario =4

union
-- Selecionar Compradores Master
Select a.IdLogin, a.Email , upper(a.Nome) as Nome from Usuarios a,
ContratoCliente b,
ContratoClienteParametros c,
(Select a.IdLogin, d.IdContrato, e.IdContratoClienteSubGrupo
From Usuarios a
inner join ContratoCliente c
on a.IdClienteResponsavel = c.IdCliente
inner join Contratos d
on c.IdContrato = d.IdContrato
inner join ContratoClienteParametros e
on c.IdContrato = e.IdContrato and
c.IdCliente = e.IdCliente
Where a.IdPerfilUsuario = 3 and a.Ativo='S' and d.Ativo = 'S' and c.ativo='S') X
where a.IdLogin = x.IdLogin And
b.IdContrato = x.IdContrato And
b.idContrato = c.idcontrato And
b.idcliente = c.idcliente And
isnull(c.IdContratoClienteSubGrupo,0) = isnull(x.IdContratoClienteSubGrupo,0)</property>
